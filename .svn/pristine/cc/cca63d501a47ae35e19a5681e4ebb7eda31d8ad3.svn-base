}
.tg_online_box a .inp::-webkit-input-placeholder{
    font-size: 0.26rem;
}
.tg_online_text{
    padding: 0.3rem 0.4rem;
}
.tg_online_text textarea{
    width: 100%;
    line-height: 0.4rem;
    color: #394043;
    font-size: 0.26rem;
    border: none;
    resize: none;
}
.online_btn{
    width: 100%;
    height: 0.98rem;
    line-height: 0.98rem;
    display: block;
    text-align: center;
    font-size: 0.28rem;
    color: #fff;
    background: #dcbe92;
    border: none;
    position: fixed;
    left: 0;
    bottom: 0;
}

