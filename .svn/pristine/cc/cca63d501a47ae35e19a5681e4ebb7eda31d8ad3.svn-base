/************** page 业主中心 提现成功 ***************/
/* 成功弹窗 */
.dialog{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 110;
    background: rgba(0,0,0,0.5);
}
.dialog_box{
    width: 6.7rem;
    /*height: 4.9rem;*/
    padding: 0 0.3rem 0.6rem;
    background: #fff;
    border-radius: 0.1rem;
    -webkit-border-radius: 0.1rem;
    position: absolute;
    top: 50%;
    margin-top: -3.6rem;
    left: 0.4rem;
    overflow: hidden;
}
.dialog_box .ico_success{
    width: 1.16rem;
    height: 1.16rem;
    display: block;
    margin: 0.58rem auto 0;
    background: url("../images/shangjia/lmshangjia-27.svg") no-repeat center;
    background-size: 100%;
}
.dialog_box h2{
    font-size: 0.32rem;
    text-align: center;
    line-height: 0.48rem;
    margin-top: 0.3rem;
    color: #394043;
}
.dialog_box p{
    padding: 0 0.3rem;
    font-size: 0.26rem;
    color: #9c9fa1;
    line-height: 0.38rem;
    text-align: center;
    margin-top: 0.15rem;
}
.dialog_btn{
    text-align: center;
    margin-top: 0.4rem;
    overflow: hidden;
    font-size: 0;
}
.dialog_btn .sure{
    width: 2.2rem;
    line-height: 0.88rem;
    font-size: 0.32rem;
    color: #fff;
    text-align: center;
    background: #6b7072;
    border-radius: 0.44rem;
    -webkit-border-radius: 0.44rem;
    display: inline-block;
    margin: 0 0.1rem;
}
.dialog_btn .close{
    width: 3.4rem;
    line-height: 0.88rem;
    font-size: 0.32rem;
    color: #fff;
    text-align: center;
