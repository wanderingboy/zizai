/*
* 业主中心
* 张展
* 2018-1-25
*/

/********** page 业主中心 **********/
/* banner */
.banner{
    width:100%;
    height: 3.6rem;
    overflow:hidden;
    position: relative;
}
.banner img{
    width: 100%;
}
.banner h2{
    width: 100%;
    font-size: 0.36rem;
    color: #fff;
    font-weight: bold;
    text-align: center;
    position: absolute;
    top: 50%;
    margin-top: -0.42rem;
    left: 0;
}
/* nav */
.owner_nav_box{
    width: 7.14rem;
    display: -webkit-box; /* Chrome 4+, Safari 3.1, iOS Safari 3.2+ */
    display: -webkit-flex; /* Chrome 21+, Safari 6.1+, iOS Safari 7+, Opera 15/16 */
    display: flex;
    margin-left: -0.22rem;
}
.owner_nav_list{
    flex: 1;
    padding: 0 0.22rem;
    border-right: 0.01rem solid #e5e5e5;
}
.owner_nav_list:last-child{
    border-right: none;
}
.owner_nav_list span{
    width: 0.96rem;
    height: 0.96rem;
    display: block;
    margin: 0 auto;
    border-radius: 100%;
    -webkit-border-radius: 100%;
}
.owner_nav_list span.ico_1{
    background: #dcbe92 url("../images/yezhu/yezhucenter-49.svg") no-repeat center;
    background-size: 0.54rem;
}
.owner_nav_list span.ico_2{
    background: #dcbe92 url("../images/yezhu/yezhucenter-50.svg") no-repeat center;
    background-size: 0.54rem;
}
.owner_nav_list span.ico_3{
    background: #dcbe92 url("../images/yezhu/yezhucenter-51.svg") no-repeat center;
    background-size: 0.54rem;
}
.owner_nav_list h2{
    font-size: 0.32rem;
    line-height: 0.32rem;
    color: #394043;
    text-align: center;
    margin-top: 0.3rem;
}
.owner_nav_list p{
    font-size: 0.2rem;
    color: #9c9fa1;
    line-height: 0.36rem;
    margin-top: 0.15rem;
    text-align: center;
}
/* owner_box1 */
.owner_box1 h2{
    font-size: 0.36rem;
    color: #394043;
    line-height: 0.36rem;
}
.owner_box1 h2 a{
    font-size: 0.28rem;
    color: #dcbe92;
    float: right;
    padding-left: 0.44rem;
    background: url("../images/yezhu/yezhucenter-52.svg") no-repeat left center;
    background-size: 0.36rem;
}
.scroll_wrap{
    width: 100%;
}
.scroll_item,.swiper-slide{
    width: 100%;
    height: 6.1rem !important;
}
.scroll_list{
    width: 100%;
    height: 100%;
    overflow: hidden;
}
.swiper-slide a{
    width: 5.38rem;
    height: 0.96rem;
    margin: 0.3rem auto 0.3rem 0.65rem;
    padding: 0.1rem 0.35rem;
    border: 0.01rem solid #e5e5e5;
    border-radius: 0.1rem;
    -webkit-border-radius: 0.1rem;
    display: -webkit-box; /* Chrome 4+, Safari 3.1, iOS Safari 3.2+ */
    display: -webkit-flex; /* Chrome 21+, Safari 6.1+, iOS Safari 7+, Opera 15/16 */
    display: flex;
    background: url("../images/index-07.svg") no-repeat right 0.2rem center;
    background-size: 0.28rem;
}
.swiper-container{
	height:6.44rem;
}
.swiper-pagination{
	}
.swiper-pagination-bullet-active{
    background: #dcbe92;
}
.swiper-slide a label{
    font-size: 0.28rem;
    color: #dcbe92;
    line-height: 0.38rem;
}
.swiper-slide a h2{
    flex: 1;
    font-size: 0.28rem;
    color: #394043;
    line-height: 0.38rem;
}

/* 新增 */
.VoteText{
    font-size: 0.24rem;
}
.VoteText em{
    color: #9c9fa1;
}
.VoteSpan{
    font-size: 0.24rem;
}
.VoteSpan em{
    color: #9c9fa1;
}
.VoteValue{
    width: 0.72rem;
    margin: 0.3rem auto 0.16rem;
}

#Demo0, #Demo1, #Demo2{
    height: 3.6rem;
    border-bottom: 0.01rem solid #e5e5e5;
    position: relative;
}
#VoteMain0, #VoteMain1, #VoteMain2{
    width: 100%;
    height: 100%;
}
.VoteItem:first-child{
    width: 1.8rem;
    position: absolute;
    left: 1.28rem;
    bottom: -0.8rem;
    text-align: center;
}
.VoteItem:last-child{
    width: 1.8rem;
    position: absolute;
    left: 3.8rem;
    bottom: -0.8rem;
}
/* 预定须知 */
.owner_box2{
    padding: 0 0.4rem;
}
.owner_box2 a{
    display: block;
    font-size: 0.28em;
    color: #394043;
    line-height: 0.88rem;
    border-bottom: 0.01rem solid #e5e5e5;
    background: url("../images/index-07.svg") no-repeat right center;
    background-size: 0.28rem;
}
.owner_box2 a:last-child{
    border-bottom: none;
}
.owner_box2 em{
    float: right;
    margin-right: 0.4rem;
}
/* 房屋推荐 */
.owner_box3{
    padding-bottom: 0;
}
.owner_box3 .title{
    margin-bottom: 0.2rem;
}
.owner_box3_item{
    padding: 0.3rem 0;
    border-top: 0.01rem solid #e5e5e5;
    display: -webkit-box; /* Chrome 4+, Safari 3.1, iOS Safari 3.2+ */
    display: -webkit-flex; /* Chrome 21+, Safari 6.1+, iOS Safari 7+, Opera 15/16 */
    display: flex;
    overflow: hidden;
}
.owner_box3_item .img{
    width: 1.6rem;
    height: 1.9rem;
    display: block;
    overflow: hidden;
    margin-right: 0.3rem;
}
.owner_box3_item .img img{
    width: 100%;
}
.owner_box3_item .info{
    flex: 1;
}
.owner_box3_item .info a.tit{
    display: block;
    width: 100%;
    padding-right: 0.4rem;
    background: url("../images/index-07.svg") no-repeat right center;
    background-size: 0.28rem;
}
.owner_box3_item .info h2{
    width: 4.4rem;
    font-size: 0.32rem;
    color: #394043;
    line-height: 0.32rem;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.owner_box3_item .info .p1{
    overflow: hidden;
    font-size: 0.24rem;
    line-height: 0.32rem;
    color: #394043;
    margin-top: 0.12rem;
}
.owner_box3_item .info .p2{
    font-size: 0.22rem;
    line-height: 0.38rem;
    color: #9c9fa1;
    margin-top: 0.1rem;
}
.owner_box3_item .info .bottom{
    margin-top: 0.21rem;
}
.owner_box3_item .info .bottom a{
    width: 1.6rem;
    height: 0.48rem;
    line-height: 0.48rem;
    text-align: center;
    float: left;
    margin-right: 0.2rem;
    font-size: 0.24rem;
    color: #394043;
    border: 0.01rem solid #e5e5e5;
    background: #f5f5f5;
    border-radius: 0.24rem;
    -webkit-border-radius: 0.24rem;
}
.owner_box3_item .info .bottom a.pay{
    background: #dcbe92;
    color: #fff;
    border: none;
}
/******* page 业主中心 托管  ********/
.owner_tg{
    padding-bottom: 1.2rem;
}
.owner_tg p{
    font-size: 0.26rem;
    color: #9c9fa1;
    line-height: 0.45rem;
    padding: 0.1rem 0;
    text-indent: 2em;
}
.owner_tg img{
    width: 100%;
    display: block;
    margin:  0.1rem 0;
}
/* 底部按钮 */
.float_wrap{
    width: 100%;
    height: 1rem;
    background: #fff;
    border-top: 0.01rem solid #e5e5e5;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 999;
    overflow: hidden;
}
.float_wrap .call{
    float: left;
    width: 2.4rem;
    height: 100%;
    text-align: center;
}
.float_wrap .call .ico_15{
    width: 0.44rem;
    height: 0.44rem;
    display: inline-block;
    margin-top: 0.14rem;
    background: url("../images/kezhan/kezhan-15.svg") no-repeat center;
    background-size: 100%;
}
.float_wrap .call p{
    font-size: 0.2rem;
    line-height: 0.4rem;
    color: #6b7072;
}
.float_wrap .pay{
    float: right;
    width: 5.1rem;
    height: 100%;
    text-align: center;
    line-height: 1rem;
    font-size: 0.32rem;
    color: #fff;
    background: #dcbe92;
}

/********* page 业主中心 托管城市 *************/
.p_tit{
    width: 100%;
    line-height: 0.6rem;
    padding: 0 0.4rem;
    font-size: 0.24rem;
    color: #9c9fa1;
}
.owner_tg_city{
    padding: 0.1rem 0.4rem 0;
}
.owner_tg_city h2{
    font-size: 0.32rem;
    color: #394043;
    line-height: 0.89rem;
}
.owner_tg_city .owner_tg_c_box{
    width: 100%;
}
.owner_tg_city .owner_tg_c_box a{
    width: 100%;
    height: 0.89rem;
    border-top: 0.01rem solid #e5e5e5;
    display: -webkit-box; /* Chrome 4+, Safari 3.1, iOS Safari 3.2+ */
    display: -webkit-flex; /* Chrome 21+, Safari 6.1+, iOS Safari 7+, Opera 15/16 */
    display: flex;
    line-height: 0.88rem;
    align-items: center;
}
.owner_tg_city .owner_tg_c_box a span{
    font-size: 0.28rem;
    color: #394043;
    margin-right: 0.1rem;
}
.owner_tg_city .owner_tg_c_box a span.city{
    padding-left: 0.4rem;
    position: relative;
}
.owner_tg_city .owner_tg_c_box a span.city::before{
    content: '';
    width: 0.18rem;
    height: 0.18rem;
    display: block;
    border-left: 0.02rem solid #e5e5e5;
    border-bottom: 0.02rem solid #e5e5e5;
    position: absolute;
    top: 0.3rem;
    left: 0;
}
.owner_tg_city .owner_tg_c_box a em{
    flex: 1;
    font-size: 0.28rem;
    color: #9c9fa1;
    text-indent: 0.3rem;
}
.owner_tg_city .owner_tg_c_box a i{
    width: 0.44rem;
    height: 0.44rem;
    background: url("../images/user/user-41.svg") no-repeat center;
    background-size: 100%;
}

/************** page 业主中心 在线托管 ***************/
.tg_online_top{
    padding: 0.25rem 0.4rem;
    border-top: none;
    margin-bottom: 0;
}
.tg_online_top p{
    padding-left: 0.6rem;
    font-size: 0.28rem;
    color: #9c9fa1;
    line-height: 0.38rem;
    background: url("../images/yezhu/yezhucenter-62.svg") no-repeat left top;
    background-size: 0.45rem;
}
.tg_online_box{
    padding:  0 0.4rem;
    margin-bottom: 0;
}
.tg_online_box a{
    width: 100%;
    height: 0.89rem;
    line-height: 0.88rem;
    border-bottom: 0.01rem solid #e5e5e5;
    display: -webkit-box; /* Chrome 4+, Safari 3.1, iOS Safari 3.2+ */
    display: -webkit-flex; /* Chrome 21+, Safari 6.1+, iOS Safari 7+, Opera 15/16 */
    display: flex;
    align-items: center;
}
.tg_online_box a:last-child{
    border-bottom: none;
}
.tg_online_box a label{
    font-size: 0.28rem;
    color: #394043;
}
.tg_online_box a em,
.tg_online_box a input{
    flex: 1;
    text-align: right;
    font-size: 0.28rem;
    color: #394043;
    padding-right: 0.4rem;
    background: url("../images/index-07.svg") no-repeat right center;
    background-size: 0.28rem;
}
.tg_online_box a span{
    flex: 1;
    text-align: right;
    font-size: 0.28rem;
    color: #9c9fa1;
}
.tg_online_box a .inp{
    flex: 1;
    text-align: right;
    font-size: 0.28rem;
    color: #394043;
    border: none;
